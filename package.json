{
  "name": "@commencis/starter-nextjs",
  "version": "1.0.0",
  "description": "Commencis Starter - NextJS",
  "author": "Commencis WEB Team",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=24.13"
  },
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typegen": "next typegen",
    "analyze": "ANALYZE=true pnpm build",
    "lint:check:types": "tsc --noEmit",
    "lint:check:js": "eslint .",
    "lint:fix:js": "eslint . --fix",
    "lint:check:css": "stylelint **/*.{css,scss}",
    "lint:fix:css": "stylelint **/*.{css,scss} --fix",
    "lint:check:format": "prettier \"**/*.{json,md}\" --check",
    "lint:fix:format": "prettier \"**/*.{json,md}\" --write",
    "lint": "pnpm run \"/^lint:check:.*/\"",
    "lint:fix": "pnpm run \"/^lint:fix:.*/\"",
    "generate:icons": "pnpm run svgr:icons && eslint --fix src/components/icons/*.{ts,tsx}",
    "generate:vectors": "pnpm run svgr:vectors && eslint --fix src/components/vectors/**/*.{ts,tsx}",
    "svgr:icons": "svgr --config-file ./scripts/svg/icon.svgr.config.cjs src/assets/icons",
    "svgr:vectors": "svgr --config-file ./scripts/svg/vector.svgr.config.cjs src/assets/vectors",
    "ci:review": "pnpm run lint",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install --no-frozen-lockfile",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@commencis/commitlint-config": "3.3.0",
    "@commencis/eslint-config": "3.5.0",
    "@commencis/lint-staged-config": "3.1.0",
    "@commencis/prettier-config": "3.1.0",
    "@commencis/stylelint-config": "3.2.0",
    "@commencis/ts-config": "1.0.0",
    "@commitlint/cli": "20.4.2",
    "@next/bundle-analyzer": "16.1.6",
    "@svgr/cli": "8.1.0",
    "@svitejs/changesets-changelog-github-compact": "1.2.0",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.3",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "stylelint": "17.3.0",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "sharp"
    ]
  }
}
